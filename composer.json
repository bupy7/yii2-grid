{
    "name": "bupy7/yii2-grid",
    "description": "Simple extended `yii\\grid\\GridView` with buns.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","module","grid","grid view"],
    "license": "BSD-3-Clause",
    "version": "1.1.2",
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "alvaro-prieto/colResizable",
                "version": "1.5",
                "source": {
                    "url": "https://github.com/alvaro-prieto/colResizable",
                    "type": "git",
                    "reference": "1.5"
                }
            }
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "npm-asset/sortablejs": "1.4.2",
        "alvaro-prieto/colResizable": "1.5"
    },
    "autoload": {
        "psr-4": {
            "bupy7\\grid\\": ""
        }
    }
}
